<div class="container">
  <h1 class="text-center">Your Lists</h1>
  <%= link_to "Create a new list!", new_list_path, class:"btn btn-primary" %>
  <div class="row">
      <% @lists.each do |list| %>
        <h2 class="name"><%= list.name %></h2>
        <%= link_to "See this list", list_path(list), class:"btn btn-primary" %>
      <% end %>
  </div>
</div>
