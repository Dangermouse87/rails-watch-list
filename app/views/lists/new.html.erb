<div class="container">
  <h1 class="title">Create a new list!</h1>
    <div class="form">
      <%= simple_form_for(@list) do |f| %>
        <%= f.input :name, label: 'List Name' %>
        <%= f.association :movies, label: 'Add a movie' %>
        <%= f.submit label: 'Create List', class:'btn btn-primary' %>
      <% end %>
    </div>
</div>
